{
  "environment" : {
      "folders": {
          "iosXibs": "Sources/App/Xibs/iOS",
          "views": "Sources/App/Views",
          "scenes": "Sources/App/ViewControllers",
          "scenesVC": "{{folders.scenes}}/{{name|firstUppercase}}",
          "scenesVM": "{{folders.scenes}}/{{name|firstUppercase}}"
      },
      "files": {
          "factory": {
              "viewModel": {
                  "items": "Sources/App/Factory/ViewModel/Item.ViewModel.Factory.swift",
                  "pickers": "Sources/App/Factory/ViewModel/Picker.ViewModel.Factory.swift",
                  "scenes": "Sources/App/Factory/ViewModel/Scene.ViewModel.Factory.swift"
              },
              "scenes": {
                  "protocols": "Sources/App/Factory/View/Scene.View.Factory.swift",
                  "implementations": {
                      "iOS": "Sources/App/Factory/View/iOS/Scene.View.Factory.Implementation.swift",
                      "tvOS": "Sources/App/Factory/View/tvOS/Scene.View.Factory.Implementation.swift"
                  }
              }
          },
          "identifiers": {
              "constants": "Sources/App/Identifiers/Constants.swift",
              "scenes": "Sources/App/Identifiers/Scene.Identifiers.swift",
              "views": "Sources/App/Identifiers/View.Identifiers.swift",
              "styles": "Sources/App/Identifiers/Style.Identifiers.swift"
          }
      },
      "targets": {
          "iOS": "Binge_iOS",
          "tvOS": "Binge_tvOS"
      }
  },
  "packages" : [
    "Configuration\/Murray\/Model\/BonePackage.json",
    "Configuration\/Murray\/App\/BonePackage.json"
  ]
